#!/bin/sh
#!SSS
#Title=Display version information
#
# [ -e /etc/version ] && cat /etc/version
echo "Accton Diagnostic Image:0b.00.01"
echo "kernel: `uname -s` `uname -r`"
echo -n "date: "
show version | grep "Build date" | cut -d " " -f 3-8

